<?xml version="1.0" encoding="UTF-8"?>
<interface name="IAuthService">
   <package>uk.ac.CO3098</package>
   <include>java.rmi.RemoteException</include>
   <include>java.io.*</include>
   <abstract_method id="login">
      <access>public</access>
      <parameterList>
         <parameter type="int">userID</parameter>
         <parameter type="String">password</parameter>
      </parameterList>
      <return>boolean</return>
   </abstract_method>
   <abstract_method id="showMessage">
      <access>public</access>
      <parameterList>
         <parameter type="int">errorCode</parameter>
         <parameter type="String">message</parameter>
      </parameterList>
      <exceptions>
         <throws>IOException</throws>
         <throws>RemoteException</throws>
      </exceptions>
      <return>void</return>
   </abstract_method>
</interface>
